{
  "name": "less-player",
  "private": true,
  "version": "0.1.15-dev-2",
  "description": "Less Player, Less is More ! ",
  "author": {
    "name": "RIVE2012",
    "email": "Rive08@126.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeekLee2012/Less-Player-Desktop.git"
  },
  "license": "Apache License 2.0",
  "main": "src/main/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev concurrently vite \"electron .\"",
    "dist": "cross-env NODE_ENV=prod vite build && electron-builder",
    "build": "cross-env NODE_ENV=prod vite build",
    "pack": "cross-env NODE_ENV=prod electron-builder"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "crypto-js": "^4.1.1",
    "hls.js": "^1.1.5",
    "howler": "^2.2.3",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.0.0",
    "mitt": "^3.0.0",
    "mousetrap": "^1.6.5",
    "music-metadata": "^7.12.3",
    "node-forge": "^1.3.1",
    "pinia": "^2.0.13",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.10.3",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14",
    "vue3-lazyload": "^0.3.6"
  },
  "devDependencies": {
    "@types/node-forge": "^1.0.1",
    "@vitejs/plugin-vue": "^2.3.0",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "electron": ">=18.0.0",
    "electron-builder": ">=22.14.13",
    "vite": "^2.9.0",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "xyz.rive2012.less-player",
    "productName": "Less Player",
    "copyright": "Copyright Â© RIVE2012",
    "beforePack": "src/main/beforePack.js",
    "directories": {
      "output": "output"
    },
    "mac": {
      "icon": "dist/icon.icns"
    },
    "win": {
      "icon": "dist/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true,
      "deleteAppDataOnUninstall": true
    },
    "linux": {
      "target": [
        {
          "target": "AppImage"
        },
        {
          "target": "deb"
        }
      ],
      "icon": "dist/icon@256X256.icns",
      "category": "Audio Video"
    }
  }
}